{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
      "object-literal-sort-keys": false,
      "max-line-length": {
        "options": [
          120
        ]
      },
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "no-console": {
        "severity": "warning",
        "options": [
          "debug",
          "info",
          "log",
          "time",
          "timeEnd",
          "trace"
        ]
      },
      "quotemark": [
        true,
        "single",
        "jsx-double"
      ],
      "ordered-imports": false,
      "interface-name": [
        true,
        "never-prefix"
      ],
      "trailing-comma": [
        true, 
        {"multiline": {
          "function": "never"
          }
        }
      ]
    },
    "jsRules": {
      "max-line-length": {
        "options": [
          120
        ]
      }
    }
  }
  